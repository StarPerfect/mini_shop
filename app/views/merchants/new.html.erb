<h1>Create A New Merchant</h1>
<%= form_tag('/merchants', method: :post) do %>
  <p><%= label_tag 'Merchant Name:' %>
  <%= text_field_tag :name %></p>
  <p><%= label_tag 'Address:' %>
  <%= text_field_tag :address %></p>
  <p><%= label_tag 'City:' %>
  <%= text_field_tag :city %></p>
  <p><%= label_tag 'State:' %>
  <%= text_field_tag :state %></p>
  <p><%= label_tag 'Zip:' %>
  <%= text_field_tag :zip %></p>
  <%= submit_tag 'Create Merchant' %>
<% end %>
